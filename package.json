    "start": "npm run dev",
    "dev": "cross-env PROXY_ENV=dev webpack-dev-server --hot --config ./scripts/webpack.dev.config.js",
    "build": "npm run build-test",
    "build-dev": "rimraf dist && cross-env ENV=devserver webpack --config ./scripts/webpack.prod.config.js --progress --env=devserver",
    "build-test": "rimraf dist && cross-env ENV=testserver webpack --config ./scripts/webpack.prod.config.js --progress --env=testserver",
    "build-uat": "rimraf dist && cross-env ENV=uatserver webpack --config ./scripts/webpack.prod.config.js --progress --env=uatserver",
    "build-www": "rimraf dist && cross-env ENV=wwwserver webpack --config ./scripts/webpack.prod.config.js --progress --env=wwwserver"
  },
  "author": "adomin",
